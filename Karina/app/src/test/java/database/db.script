CREATE MEMORY TABLE TRANSACTIONS(TRANSID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,TRANSDATE DATE,TRANSAMOUNT DOUBLE,TRANSISEXPENSE BOOLEAN NOT NULL,TRANSCATEGORYID INTEGER NOT NULL,TRANSUSERID INTEGER NOT NULL,TRANSCOMMENT VARCHAR(150),CONSTRAINT SYS_FK_50 FOREIGN KEY(TRANSUSERID) REFERENCES USERS(USERID),CONSTRAINT SYS_FK_51 FOREIGN KEY(TRANSCATEGORYID) REFERENCES CATEGORIES(CATEGORYID))
ALTER TABLE USERS ALTER COLUMN USERID RESTART WITH 21
ALTER TABLE CATEGORIES ALTER COLUMN CATEGORYID RESTART WITH 18
